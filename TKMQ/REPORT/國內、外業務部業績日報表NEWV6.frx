 AND NOT EXISTS (&#10;													SELECT 1 &#10;													FROM [TK].[dbo].[Z_SALES_DAILY_NOT_IN_COMMENTS] AS B&#10;													WHERE COPTI.TI020 LIKE '%' + B.[COMMENTS] + '%'&#10;												)&#13;&#10;			                            ) AS '銷退'&#13;&#10;		                            FROM Dates&#13;&#10;		                            LEFT JOIN [TK].[dbo].[Z_SALES_DAILY_REPORTS] ON 1=1&#13;&#10;	                            ) AS TEMP&#13;&#10;	                            WHERE NATIONS IN ('國內')&#13;&#10;	                            GROUP BY NATIONS&#13;&#10;&#13;&#10;                            ) AS TEMP2&#13;&#10;                            OPTION (MAXRECURSION 31);">
